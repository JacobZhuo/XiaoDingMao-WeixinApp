.login-box{
    margin-top:50px;
    margin-right: 60px;
    margin-left:60px;
    border:1rpx solid #D9D9D9;
    border-radius: 5px;
    position:relative;
background-color:#FFFFFF;
line-height:1.41176471;
font-size:14px;

}
.login-cell {
  padding: 0px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;          
}
.login-cell:before {
  content: " ";
  position: absolute;
  left: 15px;
  top: 0;
  right: 15px;
  height: 1px;
  border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;  
}
.login-cell:first-child:before {
  display: none;
}
.text-box{
padding: 10px 30px;

font-size:14px;
text-align: center;

}
.login-btn-box{
    margin:0 60px;
    padding:15px 0px;
}
.login-btn{
    font-size:15px;
}
.login-signup{
    margin: 0 30px;
    padding:0 15px;
    text-align: center;
    color: #D9D9D9;
}
